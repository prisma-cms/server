{
  "name": "@prisma-cms/server",
  "version": "1.1.5",
  "description": "",
  "main": "lib/index.js",
  "module": "es/index.js",
  "files": [
    "src",
    "es",
    "lib",
    "umd"
  ],
  "scripts": {
    "build": "nwb build-web-module",
    "clean": "nwb clean-module",
    "prepublishOnly": "npm run build",
    "test": "nwb test",
    "test:coverage": "nwb test --coverage",
    "test:watch": "nwb test --server",
    "start": "nodemon src/index.js",
    "start-server": "Sendmail=true action=start-server node src/index.js",
    "start-server-dev": "Sendmail=true SendmailTest=true action=start-server yarn start",
    "deploy": "run-s build-schema-prisma && yarn deploy-schema && yarn get-schema -p prisma && yarn build-schema-api",
    "deploy-force": "run-s build-schema-prisma && yarn deploy-schema -f && yarn get-schema -p prisma && yarn build-schema-api",
    "deploy-prod": "yarn deploy",
    "build-schema-prisma": "schemaType=prisma yarn build-schema",
    "deploy-schema": "node_modules/prisma/dist/index.js deploy",
    "get-schema": "node_modules/graphql-cli/dist/bin.js get-schema -p prisma -o src/schema/generated/prisma.graphql",
    "build-schema-api": "schemaType=api yarn build-schema",
    "build-schema": "action=build-schema node src/index.js",
    "voyager-prisma": "node_modules/graphql-cli/dist/bin.js voyager -p prisma",
    "voyager-app": "node_modules/graphql-cli/dist/bin.js voyager -p app"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "geoip-lite": "^1.3.2",
    "graphql-cli": "^2.16.5",
    "graphql-import": "^0.6.0",
    "graphql-yoga": "^1.16.1",
    "html-to-json": "^0.6.0",
    "iconv": "^2.3.0",
    "isemail": "^3.1.3",
    "knex": "^0.15.2",
    "md5": "^2.2.1",
    "merge-graphql-schemas": "^1.5.3",
    "mysql": "^2.16.0",
    "prisma": "^1.16.0",
    "prisma-binding": "^2.1.5",
    "promises-all": "^1.0.0",
    "random-useragent": "^0.3.1",
    "react-cms-graphql-utils": "^1.1.0",
    "sendmail": "^1.4.1",
    "sharp": "^0.20.7",
    "shortid": "^2.2.13",
    "urijs": "^1.19.1"
  },
  "devDependencies": {
    "babel-eslint": "^7.0.0",
    "babel-plugin-transform-class-properties": "^6.11.5",
    "babel-plugin-transform-runtime": "^6.12.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "babel-register": "^6.26.0",
    "nwb": "0.23.x"
  },
  "author": "",
  "license": "MIT",
  "repository": ""
}
